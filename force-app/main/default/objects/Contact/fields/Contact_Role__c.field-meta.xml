<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_Role__c</fullName>
    <description>Identifies whether the contact participates as a Trainer or a Trainee to drive flow branching and assignment logic.</description>
    <inlineHelpText>Select Trainer when the contact delivers courses; select Trainee when the contact enrolls in courses.</inlineHelpText>
    <label>Contact Role</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Trainer</fullName>
                <default>false</default>
                <label>Trainer</label>
            </value>
            <value>
                <fullName>Attendee</fullName>
                <default>false</default>
                <label>Attendee</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
